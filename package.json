  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "rm -rf dist .expo node_modules/.cache",

    "export:web": "npx expo export --platform web --output-dir dist --clear && cp -f favicon.png dist/favicon.png || true && cp -f apple-touch-icon.png dist/apple-touch-icon.png || true",

    "deploy:web": "npm run export:web && vercel deploy --prod dist"
  },
