{
  "routes": [
    {
      "src": "/auth/v1/verify",
      "dest": "/index.html"
    },
    {
      "src": "/auth/confirm",
      "dest": "/index.html"
    },
    {
      "src": "/reset-password",
      "dest": "/index.html"
    },
    {
      "src": "/signin",
      "dest": "/index.html"
    },
    {
      "src": "/signup",
      "dest": "/index.html"
    },
    {
      "src": "/profile/.*",
      "dest": "/index.html"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
